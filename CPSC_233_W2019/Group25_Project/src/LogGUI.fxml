<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0"
            prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="LogGUI">
    <Label layoutX="27.0" layoutY="20.0" prefHeight="21.0" prefWidth="150.0"
           text="History Log">
        <font>
            <Font name="Arial Bold" size="18.0"/>
        </font>
    </Label>
    <Button layoutX="203.0" layoutY="646.0" mnemonicParsing="false"
            onAction="#searchOnAction" prefHeight="27.0" prefWidth="92.0"
            text="Search">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <TextField fx:id="keywordTextField" layoutX="27.0" layoutY="646.0"
               onKeyReleased="#keywordKeyReleased" prefHeight="27.0"
               prefWidth="157.0"/>
    <Label layoutX="27.0" layoutY="611.0" prefHeight="21.0" prefWidth="92.0"
           text="Keyword">
        <font>
            <Font name="Arial Italic" size="18.0"/>
        </font>
    </Label>
    <TextArea fx:id="historyLogTextArea" editable="false" layoutX="29.0"
              layoutY="53.0" prefHeight="548.0" prefWidth="1140.0"
              wrapText="true">
        <font>
            <Font name="Monaco" size="14.0"/>
        </font>
    </TextArea>
    <Button layoutX="307.0" layoutY="646.0" mnemonicParsing="false"
            onAction="#resetOnAction" prefHeight="27.0" prefWidth="92.0"
            text="Reset">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
